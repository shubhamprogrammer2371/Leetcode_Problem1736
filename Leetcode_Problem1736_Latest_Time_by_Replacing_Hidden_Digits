char* maximumTime(char* time) {
    if(time[0] == '?')
        time[0] = ((time[1] == '3') || (time[1] == '2') || (time[1] == '1') || (time[1] == '0') || (time[1] == '?')) ? '2' : '1';
    if(time[1] == '?')
        time[1] = ((time[0] == '2') || (time[0] == '?'))? '3' : '9';
    time[3] = (time[3] == '?') ? '5' : time[3];
    time[4] = (time[4] == '?') ? '9' : time[4];
    return time;
}


--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


char * maximumTime(char * time){
    if (time[0] == '?' && time[1] == '?') 
    {
        time[0] = '2';
        time[1] = '3';
    } 
    else if (time[0] == '?') 
        time[0] = (time[1] <= '3') ? '2' : '1';
    else if (time[1] == '?') 
        time[1] = (time[0] == '2') ? '3' : '9';
    if (time[3] == '?') 
        time[3] = '5';
    if (time[4] == '?') 
        time[4] = '9';
    return time;
}


--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


char * maximumTime(char * time){
    for (int i = 0; time[i] != '\0'; i++) 
    {
        if (i == 0 && time[i] == '?') 
        {
            if (time[i+1] == '1' || time[i+1] == '2' || time[i+1] == '3' || time[i+1] == '0' || time[i+1] == '?')
                time[i] = '2';
            else 
                time[i] = '1';
        }
        else if (i == 1 && time[i] == '?') 
        {
            if (time[i-1] == '2')
                time[i] = '3';
            else
                time[i] = '9';
        }
        else if (i == 3 && time[i] == '?')
            time[i] = '5';
        else if (i == 4 && time[i] == '?')
            time[i] = '9';
    }
    return time;
}
